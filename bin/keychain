GITHUB_TOKEN=$(security find-internet-password -a GITHUB_TOKEN -s GITHUB_TOKEN -w)
export GITHUB_TOKEN

GITHUB_USER=$(security find-internet-password -a GITHUB_USER -s GITHUB_USER -w)
export GITHUB_USER

TWINE_USERNAME=$(security find-internet-password -a TWINE_USERNAME -s TWINE_USERNAME -w)
export TWINE_USERNAME

TWINE_PASSWORD=$(security find-internet-password -a TWINE_PASSWORD -s TWINE_PASSWORD -w)
export TWINE_PASSWORD

DOCKER_USERNAME=$(security find-internet-password -a DOCKER_USERNAME -s DOCKER_USERNAME -w)
export DOCKER_USERNAME

DOCKER_PASSWORD=$(security find-internet-password -a DOCKER_PASSWORD -s DOCKER_PASSWORD -w)
export DOCKER_PASSWORD

AWS_ACCESS_KEY_ID=$(security find-internet-password -a AWS_ACCESS_KEY_ID -s AWS_ACCESS_KEY_ID -w)
export AWS_ACCESS_KEY_ID

AWS_SECRET_ACCESS_KEY=$(security find-internet-password -a AWS_SECRET_ACCESS_KEY -s AWS_SECRET_ACCESS_KEY -w)
export AWS_SECRET_ACCESS_KEY
